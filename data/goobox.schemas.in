        <type>string</type>
        <default>ascending</default>
        <locale name="C">
	  <short></short>
	  <long></long>
        </locale>
      </schema>

      <!-- EXTRACT -->

      <schema>
	<key>/schemas/apps/goobox/dialogs/extract/destination</key>
	<applyto>/apps/goobox/dialogs/extract/destination</applyto>
	<owner>goobox</owner>
	<type>string</type>
	<default></default>
	<locale name="C">
	  <short></short>
	  <long> 
	  </long>
	</locale>
      </schema>

      <schema>
	<key>/schemas/apps/goobox/dialogs/extract/file_type</key>
