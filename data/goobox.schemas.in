	<key>/schemas/apps/goobox/playlist/repeat</key>
	<applyto>/apps/goobox/playlist/repeat</applyto>
	<owner>goobox</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	  <short></short>
	  <long>
	  </long>
