	<applyto>/apps/goobox/dialogs/ripper/view_destination</applyto>
	<owner>goobox</owner>
	<type>bool</type>
	<default>false</default>
	<locale name="C">
	  <short></short>
	  <long>
	  </long>
	</locale>
      </schema>

      <!-- ENCODER -->

      <schema>
	<key>/schemas/apps/goobox/encoder/ogg_quality</key>
	<applyto>/apps/goobox/encoder/ogg_quality</applyto>
	<owner>goobox</owner>
	<type>float</type>
	<default>0.3</default>
	<locale name="C">
	  <short></short>
